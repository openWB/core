{
  "name": "colordisplaytheme2",
  "version": "0.9.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --base=/openWB/web/display/themes/colors --outDir=../web --emptyOutDir",
